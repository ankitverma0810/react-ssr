{
	"name": "react-ssr",
	"version": "1.0.0",
	"description": "Server side rendering project",
	"main": "index.js",
	"node": "10.16.x",
	"scripts": {
		"clean": "rimraf build && rimraf public && mkdir build && mkdir public",
		"start": "npm run clean && npm-run-all --parallel dev:*",
		"dev:build-server": "webpack --config webpack.server.js --watch",
		"dev:build-client": "webpack --config webpack.client.js --watch",
		"dev:server": "touch build/main.js && nodemon --watch build --exec node build/main.js",
		"build": "npm run clean && npm-run-all --parallel prod:*",
		"prod:build-server": "cross-env NODE_ENV=production webpack --config webpack.server.js",
		"prod:build-client": "cross-env NODE_ENV=production webpack --config webpack.client.js",
		"test": "jest --watch"
	},
	"jest": {
		"transform": {
			"^.+\\.js$": "<rootDir>/node_modules/babel-jest"
		},
		"setupFiles": [
			"./src/client/setupTests.js"
		]
	},
	"author": "Ankit Verma",
	"license": "ISC",
	"dependencies": {
		"@babel/polyfill": "^7.11.5",
		"@loadable/component": "^5.10.3",
		"@loadable/server": "^5.10.3",
		"@material-ui/core": "^4.11.0",
		"axios": "^0.20.0",
		"compression": "^1.7.4",
		"concurrently": "3.5.0",
		"core-js": "^3.6.5",
		"express": "^4.17.1",
		"express-http-proxy": "1.0.6",
		"jss": "^10.4.0",
		"lodash": "^4.17.20",
		"nodemon": "1.12.0",
		"npm-run-all": "4.1.1",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-helmet": "^6.1.0",
		"react-jss": "^10.4.0",
		"react-redux": "^7.2.0",
		"react-router-config": "^5.1.1",
		"react-router-dom": "^5.1.2",
		"redux": "^4.0.5",
		"redux-thunk": "2.2.0",
		"serialize-javascript": "1.4.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.4.4",
		"@babel/core": "^7.6.2",
		"@babel/node": "^7.0.0",
		"@babel/plugin-proposal-class-properties": "^7.12.1",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.11.5",
		"@babel/preset-env": "^7.6.2",
		"@babel/preset-react": "^7.0.0",
		"@loadable/babel-plugin": "^5.10.3",
		"@loadable/webpack-plugin": "^5.7.1",
		"babel-jest": "^26.6.1",
		"babel-loader": "^8.0.6",
		"babel-plugin-styled-components": "^1.11.1",
		"check-prop-types": "^1.1.2",
		"cross-env": "^7.0.2",
		"css-loader": "^2.1.1",
		"dotenv": "^8.2.0",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.5",
		"jest": "^26.6.1",
		"jest-cli": "^26.6.1",
		"jest-enzyme": "^7.1.2",
		"mini-css-extract-plugin": "^0.6.0",
		"optimize-css-assets-webpack-plugin": "^5.0.4",
		"prop-types": "^15.7.2",
		"redux-mock-store": "^1.5.4",
		"style-loader": "^2.0.0",
		"terser-webpack-plugin": "^4.2.2",
		"webpack": "^4.44.2",
		"webpack-cli": "^3.3.12",
		"webpack-dev-server": "^3.11.0",
		"webpack-merge": "^5.1.4",
		"webpack-node-externals": "^2.5.2"
	}
}
